  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CoCreate-app/CoCreate-nginx/issues"
  },
  "homepage": "https://cocreate.app/docs/nginx",
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/CoCreate-app"
  },
  "main": "./src/index.js"
}
